{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev:web": "nodemon src/workers/web.js",
    "dev:synchronize": "nodemon src/workers/synchronize.js",
    "dev:build": "nodemon src/workers/build.js",
    "build": "babel -d lib src --source-maps inline",
    "run:web": "node lib/workers/web.js",
    "run:synchronize": "node lib/workers/synchronize.js",
    "run:build": "node lib/workers/build.js",
    "db:check-structure": "knex-scripts check-structure",
    "db:create": "yarn knex-scripts create",
    "db:drop": "yarn knex-scripts drop",
    "db:dump": "yarn db:migrate && yarn knex-scripts dump",
    "db:load": "yarn knex-scripts load",
    "db:seed": "yarn knex seed:run",
    "db:truncate": "yarn knex-scripts truncate",
    "db:migrate": "yarn knex migrate:latest",
    "db:rollback": "yarn knex migrate:rollback",
    "db:reset": "yarn db:drop && yarn db:create && yarn db:migrate",
    "knex": "babel-node ./node_modules/.bin/knex",
    "knex-scripts": "babel-node ./node_modules/.bin/knex-scripts",
    "smee": "smee --url https://smee.io/8hyGlwoDt5131zV --path /event-handler --port 3000"
  },
  "dependencies": {
    "@octokit/app": "^4.1.0",
    "@octokit/rest": "^16.33.0",
    "@octokit/webhooks": "^6.3.2",
    "@sentry/node": "^5.7.0",
    "ajv": "^6.10.2",
    "amqplib": "^0.5.5",
    "apollo-server-express": "^2.14.2",
    "aws-sdk": "^2.547.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "bytes": "^3.1.0",
    "chalk": "^2.4.2",
    "convict": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-err": "^3.0.0",
    "filesize": "^5.0.3",
    "graphql": "^14.5.8",
    "graphql-iso-date": "^3.6.1",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.5",
    "gzip-size": "^5.1.1",
    "knex": "^0.19.5",
    "knex-scripts": "^0.3.1",
    "markdown-table": "^1.1.3",
    "merge-deep": "^3.0.2",
    "minimatch": "^3.0.4",
    "objection": "^1.6.11",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "pg": "^7.12.1"
  },
  "devDependencies": {
    "factory-girl": "^5.0.4",
    "factory-girl-objection-adapter": "^1.0.0"
  }
}
